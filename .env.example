# LLM Provider Configuration
# Supported providers: claude, chatgpt (or openai), gemini (or google)
LLM_PROVIDER=claude

# Anthropic Claude API Configuration
ANTHROPIC_API_KEY=your_anthropic_api_key_here
CLAUDE_MODEL=claude-sonnet-4-5-20250929

# Alternative Claude models:
# CLAUDE_MODEL=claude-opus-4-5-20251101
# CLAUDE_MODEL=claude-3-5-sonnet-20241022
# CLAUDE_MODEL=claude-3-opus-20240229

# OpenAI ChatGPT API Configuration (if using LLM_PROVIDER=chatgpt)
# OPENAI_API_KEY=your_openai_api_key_here
# OPENAI_MODEL=gpt-4o
# Alternative OpenAI models:
# OPENAI_MODEL=gpt-4-turbo
# OPENAI_MODEL=gpt-3.5-turbo

# Google Gemini API Configuration (if using LLM_PROVIDER=gemini)
# GOOGLE_API_KEY=your_google_api_key_here
# GEMINI_MODEL=gemini-2.0-flash-exp
# Alternative Gemini models:
# GEMINI_MODEL=gemini-1.5-pro
# GEMINI_MODEL=gemini-1.5-flash

# Caldera Configuration
CALDERA_URL=http://localhost:8888
CALDERA_API_KEY=ADMIN123

VBOX_HOST=http://localhost:8888
VBOX_USERNAME=local
VBOX_PASSWORD=your_password_here

VBOX_VM_NAME=test
VBOX_SNAPSHOT_NAME=test